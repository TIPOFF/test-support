{
    "name": "tipoff/test-support",
    "description": "PHP Package for test support on other packages",
    "keywords": [
        "tipoff",
        "test-support"
    ],
    "homepage": "https://github.com/tipoff/test-support",
    "license": "MIT",
    "authors": [
        {
            "name": "Tipoff",
            "email": "packages@tipoff.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "mockery/mockery": "^1.4",
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.3",
        "spatie/laravel-ray": "^1.9",
        "tipoff/authorization": "^2.0.0",
        "tipoff/support": "^1.4.10",
        "vimeo/psalm": "^4.4"
    },
    "autoload": {
        "psr-4": {
            "Tipoff\\TestSupport\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tipoff\\TestSupport\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit --colors=always",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
